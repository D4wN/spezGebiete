jade:   meteor add mquandalle:jade
db:     meteor add aldeed:simple-schema
        meteor add aldeed:collection2
bootstrap: meteor add mizzao:bootstrap-3

aggregation support:
        meteor add meteorhacks:aggregate

//vorher meteor starten >_>
db fill:        mongoimport --file mail.json --db meteor --port 3001

meteor db ul:   process.env.MONGO_URL



